{
    "hosts-path": "/etc/hosts",
    "php-cli-version": "7.4",
    "composer-version": "2",
    "folders": [
        {
            "source": "~/Desktop/project",
            "dist": "/var/www/project"
        }
    ],
    "sites": [
        {
            "domain": "phpmyadmin.test",
            "entry-point": "/var/www/project/phpmyadmin",
            "template": "default.conf",
            "php-fpm-version": "7.4",
            "auto_host": true
        }
    ],
    "ports": {
        "80": "80",
        "443": "443",
        "3306": "3306",
        "6379": "6379"
    }
}
